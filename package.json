{
  "name": "automation-business-suite",
  "version": "1.0.0",
  "description": "Complete automation business toolkit - $30K+ revenue potential",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:css && npm run build:js",
    "build:css": "sass src/scss:public/css --style=compressed",
    "build:js": "webpack --mode=production",
    "install-deps": "npm install && cd client && npm install",
    "demo": "node start-demo.js",
    "demo-setup": "node demo-setup.js",
    "demo-deploy": "node deploy-demo.js",
    "demo-build": "cd client && npm run build"
  },
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.3",
